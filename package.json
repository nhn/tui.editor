{
  "name": "tui-editor",
  "version": "1.0.5",
  "description": "GFM  Markdown Wysiwyg Editor - Productive and Extensible",
  "keywords": [
    "toast",
    "ui",
    "tui",
    "markdown",
    "wysiwyg",
    "editor",
    "preview",
    "gfm",
    "jquery-plugin"
  ],
  "homepage": "https://github.com/nhnent/tui.editor",
  "bugs": "https://github.com/nhnent/tui.editor/issues",
  "author": "NHN Ent. FE Development Lab <dl_javascript@nhnent.com> (https://nhnent.github.io/tui.editor/)",
  "repository": "https://github.com/nhnent/tui.editor.git",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "dist/tui-editor-Editor.js",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.1.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-istanbul": "^4.1.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "clean-css": "^4.1.9",
    "copy-webpack-plugin": "^4.5.1",
    "cross-env": "^5.1.3",
    "eslint": "^4.14.0",
    "eslint-config-tui": "^1.0.1",
    "eslint-loader": "^1.7.1",
    "istanbul-instrumenter-loader": "^3.0.0",
    "jasmine-core": "^2.5.2",
    "jsdoc": "^3.5.5",
    "karma": "^2.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^1.0.1",
    "karma-coverage": "^1.1.1",
    "karma-edge-launcher": "^0.4.2",
    "karma-firefox-launcher": "^1.1.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-jasmine": "^1.1.1",
    "karma-jasmine-ajax": "^0.1.13",
    "karma-jasmine-jquery": "^0.1.1",
    "karma-junit-reporter": "^1.2.0",
    "karma-narrow-reporter": "^2.0.1",
    "karma-remap-coverage": "^0.1.4",
    "karma-safari-launcher": "^1.0.0",
    "karma-sauce-launcher": "^1.2.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webdriver-launcher": "github:nhnent/karma-webdriver-launcher#v1.1.0",
    "karma-webpack": "^2.0.9",
    "safe-umd-webpack-plugin": "0.0.2",
    "tui-jsdoc-template": "^1.2.2",
    "tui-release-notes": "github:nhnent/tui.release-notes#v1.0.0",
    "webpack": "^3.10.0",
    "webpack-bundle-analyzer": "^2.9.1",
    "webpack-dev-server": "^2.9.7"
  },
  "scripts": {
    "lint": "eslint .",
    "doc": "npm run doc:copy & npm run doc:jsdoc",
    "doc:copy": "VER=$(cat package.json | grep version | head -1 | awk -F: '{ print $2 }' | sed 's/[\",]//g' | tr -d '[[:space:]]') && copyfiles \"dist/**/*\" \"api/$VER\" && copyfiles \"lib/**/*\" \"api/$VER\"",
    "doc:jsdoc": "jsdoc -c jsdoc.conf.js",
    "test": "karma start",
    "test:ne": "cross-env SERVER=nhnent karma start",
    "test:sl": "cross-env SERVER=saucelabs karma start",
    "test:lh": "cross-env SERVER=localhost karma start",
    "serve": "webpack-dev-server --progress --inline",
    "build": "cross-env webpack && webpack -p",
    "note": "tuie --tag=$(git describe --tags)"
  },
  "dependencies": {
    "codemirror": "^5.33.0",
    "highlight.js": "^9.12.0",
    "jquery": "^3.3.1",
    "markdown-it": "^8.4.0",
    "plantuml-encoder": "^1.2.5",
    "squire-rte": "^1.8.11",
    "to-mark": "^1.1.0",
    "tui-chart": "^2.13.0",
    "tui-code-snippet": "^1.2.9",
    "tui-color-picker": "^2.0.1"
  }
}
